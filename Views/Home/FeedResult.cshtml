@model List<CodeHollow.FeedReader.FeedItem>

<h1>RSS Feed Results</h1>

@if (Model != null && Model.Any())
{
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <h3>@item.Title</h3>
                 <div>@Html.Raw(item.Description)</div>  <!-- Render HTML content -->
                <a href="@item.Link" target="_blank">Read more</a>
            </li>
        }
    </ul>
}
else
{
    <p>No feed items available.</p>
}

<a href="/Home/Index">Go back</a>